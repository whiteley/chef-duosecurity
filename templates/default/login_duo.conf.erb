[duo]
ikey = <%= @integration_key %>
skey = <%= @secret_key %>
host = <%= @api_hostname %>

<% if @groups %>
groups = <%= @groups %>
<% end %>

failmode = <%= @failmode %>
pushinfo = <%= @pushinfo %>

<% if @http_proxy %>
http_proxy = <%= @http_proxy %>
<% end %>

autopush = <%= @autopush %>
motd = <%= @motd %>
prompts = <%= @prompts %>
accept_env_factor = <%= @accept_env_factor %>
fallback_local_ip = <%= @fallback_local_ip %>
https_timeout = <%= @https_timeout %>
